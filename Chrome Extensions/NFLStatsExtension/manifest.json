{
    "name": "NFL Data Scraper",
    "description": "Scrape NFL data",
    "version": "1",
    "permissions": [
        "tabs","<all_urls>",
        "storage"
    ],
    "content_scripts": [
        {
        "matches": [
            "https://www.nfl.com/stats/player-stats*"
        ],
        "js": ["jquery-3.5.1.min.js", "script.js"]
        }
    ],
    "manifest_version": 2
}